{
  "name": "@atakama/cover-diff",
  "version": "1.2.3",
  "description": "coverage reporter and enforcer for diffs only",
  "main": "cover-diff.js",
  "bin": {
    "cover-diff": "./cli.js"
  },
  "scripts": {
    "lint": "eslint *.js",
    "test": "node test.js",
    "coverage": "nyc --reporter lcov --reporter text-summary yarn test"
  },
  "files": [
    "*.md",
    "*.js"
  ],
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "branches": 70
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtakamaLLC/cover-diff.git"
  },
  "keywords": [
    "coverage",
    "diffs",
    "diff"
  ],
  "author": "Erik Aronesty",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AtakamaLLC/cover-diff/issues"
  },
  "homepage": "https://github.com/AtakamaLLC/cover-diff#readme",
  "dependencies": {
    "find-up": "^4.1.0",
    "get-stdin": "^8.0.0",
    "lcov-parse": "^1.0.0",
    "parse-diff": "^0.7.0"
  },
  "devDependencies": {
    "@atakama/qtest": "^1.6.0",
    "eslint": "^7.3.1"
  },
  "@atakama/cover-diff": {
    "lines": 95,
    "branches": 90
  }
}
